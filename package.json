{
  "name": "web-server",
  "version": "1.0.0",
  "description": "Web server from Himalayan Hike",
  "main": "index.js",
  "scripts": {
    "start": "MODE=PROD node build/bundle.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:run": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:server": "webpack --mode development --config webpack.server.js --watch",
    "dev:client": "webpack --mode development --config webpack.client.js --watch",
    "build": "npm-run-all --parallel build:*",
    "build:server": "webpack --mode production --config webpack.server.js",
    "build:client": "webpack --mode production --config webpack.client.js"
  },
  "author": "Manjil Tamang",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "query-string": "^6.11.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.4",
    "react-redux": "^7.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "html-webpack-plugin": "^4.0.3",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^8.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
